        android:textColor="#FFFFFF"
        android:textSize="15sp"
        android:textStyle="bold" />

    <Button
        android:id="@+id/button02"
        android:layout_width="337dp"
        android:layout_height="36dp"
        android:layout_alignParentTop="true"
